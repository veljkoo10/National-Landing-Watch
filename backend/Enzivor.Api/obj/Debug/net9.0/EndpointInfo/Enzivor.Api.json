{
  "openapi": "3.0.4",
  "info": {
    "title": "Enzivor.Api",
    "version": "1.0"
  },
  "paths": {
    "/api/Landfills/process": {
      "post": {
        "tags": [
          "Landfills"
        ],
        "parameters": [
          {
            "name": "persist",
            "in": "query",
            "schema": {
              "type": "boolean",
              "default": true
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ProcessLandfillsRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ProcessLandfillsRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ProcessLandfillsRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Landfills/detections": {
      "get": {
        "tags": [
          "Landfills"
        ],
        "parameters": [
          {
            "name": "minConfidence",
            "in": "query",
            "schema": {
              "type": "number",
              "format": "double"
            }
          },
          {
            "name": "type",
            "in": "query",
            "schema": {
              "$ref": "#/components/schemas/LandfillCategory"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LandfillCategory": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "ProcessLandfillsRequest": {
        "type": "object",
        "properties": {
          "csvPath": {
            "type": "string",
            "nullable": true
          },
          "northWestLat": {
            "type": "number",
            "format": "double"
          },
          "northWestLon": {
            "type": "number",
            "format": "double"
          },
          "southEastLat": {
            "type": "number",
            "format": "double"
          },
          "southEastLon": {
            "type": "number",
            "format": "double"
          },
          "imageWidthPx": {
            "type": "integer",
            "format": "int32"
          },
          "imageHeightPx": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    }
  }
}